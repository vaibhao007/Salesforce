    <validationRules>
        <fullName>Close_Oppty_for_Prospect</fullName>
        <active>true</active>
        <description>Prevents users from moving opportunities to Closed Won status for prospect accounts.</description>
        <errorConditionFormula>AND( ISPICKVAL( StageName , &apos;Closed Won&apos;) , Account.RecordType.DeveloperName  = &apos;Prospect&apos;)</errorConditionFormula>
        <errorMessage>You cannot close Opportunities that have been awarded against Prospect Accounts. Please contact the CMF team to create an Account in MDM.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OpportunityReqFieldsforProbability</fullName>
        <active>false</active>
        <errorConditionFormula>AND((Probability&gt;0.30),(RecordType.DeveloperName =&quot;OM_RFP&quot;	),ISBLANK(OM_Delivery_Method__c),
ISBLANK( OM_Proposed_Services__c ),
ISBLANK(  Product__c ))</errorConditionFormula>
        <errorMessage>Following Fields should have a value for Probability Greater than 30%-
Delivery Method,
Proposed Services,
Product</errorMessage>
    </validationRules>
</CustomObject>
