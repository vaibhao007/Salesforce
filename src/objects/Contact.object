    <validationRules>
        <fullName>ContactEmailRequired</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   ISPICKVAL( OM_Preferred_Contact_Method__c , &apos;Email&apos;), 
   ISBLANK(Email) 
)</errorConditionFormula>
        <errorDisplayField>Email</errorDisplayField>
        <errorMessage>Please provide Email Address</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContactFaxRequired</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL( OM_Preferred_Contact_Method__c , &apos;Fax&apos;), 
ISBLANK( Fax ) 
)</errorConditionFormula>
        <errorDisplayField>Fax</errorDisplayField>
        <errorMessage>Please provide Fax Details</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContactMailRequired</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
ISPICKVAL( OM_Preferred_Contact_Method__c , &apos;Mail&apos;), 
ISBLANK( MailingAddress ) 
)</errorConditionFormula>
        <errorDisplayField>MailingStreet</errorDisplayField>
        <errorMessage>Please Enter the mailing address in the Mailing Address field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContactMailingCityRequired</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
ISPICKVAL( OM_Preferred_Contact_Method__c , &apos;Mail&apos;),  ISBLANK ( MailingCity ) 
)</errorConditionFormula>
        <errorDisplayField>MailingCity</errorDisplayField>
        <errorMessage>Please enter the mailing city</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContactMailingPostalCodeRequired</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
ISPICKVAL( OM_Preferred_Contact_Method__c , &apos;Mail&apos;),  ISBLANK (  MailingPostalCode  ) 
)</errorConditionFormula>
        <errorDisplayField>MailingPostalCode</errorDisplayField>
        <errorMessage>Please enter the Postal Code</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContactMobileRequired</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL( OM_Preferred_Contact_Method__c , &apos;Mobile Phone&apos;), 
ISBLANK( MobilePhone ) 
)</errorConditionFormula>
        <errorDisplayField>MobilePhone</errorDisplayField>
        <errorMessage>Please enter the appropriate mobile number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContactPhoneRequired</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL( OM_Preferred_Contact_Method__c , &apos;Phone&apos;), 
ISBLANK( Phone ) 
)</errorConditionFormula>
        <errorDisplayField>Phone</errorDisplayField>
        <errorMessage>Please enter Phone Details</errorMessage>
    </validationRules>
